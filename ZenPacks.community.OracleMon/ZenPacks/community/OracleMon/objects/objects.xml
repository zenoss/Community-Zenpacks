<object id='session' module='ZenPacks.community.OracleMon.datasources.OracleDataSource' class='OracleDataSource'>
<property select_variable="sourcetypes" type="selection" id="sourcetype" mode="w" >
Oracle
</property>
<property type="boolean" id="enabled" mode="w" >
True
</property>
<property type="int" id="severity" mode="w" >
3
</property>
<property type="int" id="cycletime" mode="w" >
300
</property>
<property type="string" id="sql" mode="w" >
SELECT sum(abp) ActiveBackgroundProcesses,
       sum(asp) ActiveSystemProcesses,
       sum(aup) ActiveUserProcesses,
       sum(tbp) TotalBackgroundProcesses,
       sum(tsp) TotalSystemProcesses,
       sum(tup) TotalUserProcesses
FROM   (
       SELECT decode(st, 'bp', sum(status), 0) abp,
              decode(st, 'bp', count(*), 0) tbp,
              decode(st, 'sp', sum(status), 0) asp,
              decode(st, 'sp', count(*), 0) tsp,
              decode(st, 'up', sum(status), 0) aup,
              decode(st, 'up', count(*), 0) tup
        FROM (
            SELECT
                CASE
                    WHEN username IS null
                    THEN 'bp'
                    WHEN username IN ('SYSTEM', 'SYSMAN', 'SYS', 'DBSNMP')
                    THEN 'sp'
                    ELSE 'up'
                END AS st,
                decode(status, 'ACTIVE', 1, 0) status
                FROM v$$session)
        GROUP BY st)
</property>
<property type="string" id="dsn" mode="w" >
${here/dsn}
</property>
<property type="string" id="username" mode="w" >
${here/zOracleUser}
</property>
<property type="string" id="password" mode="w" >
${here/zOraclePassword}
</property>
<property type="int" id="timeout" mode="w" >
15
</property>
