<tomanycont id='datasources'>
<object id='sizeUsed' module='ZenPacks.community.OracleMon.datasources.OracleDataSource' class='OracleDataSource'>
<property select_variable="sourcetypes" type="selection" id="sourcetype" mode="w" >
Oracle
</property>
<property type="boolean" id="enabled" mode="w" >
True
</property>
<property type="int" id="severity" mode="w" >
3
</property>
<property type="int" id="cycletime" mode="w" >
300
</property>
<property type="string" id="sql" mode="w" >
SELECT     b.BLOCKS sizeUsed,
               a.BYTES totalBytes,
               TABLESPACE_NAME
FROM
        (
                SELECT     TABLESPACE_NAME,
                                sum(BYTES) BYTES
                FROM        dba_data_files
                GROUP BY TABLESPACE_NAME
        ) a
LEFT JOIN
        (
                SELECT     TABLESPACE_NAME,
                                sum(BLOCKS) BLOCKS
                FROM        dba_free_space
                GROUP BY TABLESPACE_NAME
        ) b
USING       (TABLESPACE_NAME)
WHERE      TABLESPACE_NAME='${here/dbname}'
</property>
<property type="string" id="dsn" mode="w" >
${here/dsn}
</property>
<property type="string" id="username" mode="w" >
${here/zOracleUser}
</property>
<property type="string" id="password" mode="w" >
${here/zOraclePassword}
</property>
<property type="int" id="timeout" mode="w" >
15
</property>
