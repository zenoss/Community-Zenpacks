<object id='/zport/dmd/Devices/Server/rrdTemplates/OracleTablespace' module='Products.ZenModel.RRDTemplate' class='RRDTemplate'>
<property type="text" id="description" mode="w" >
Oracle Tablespace monitoring template
</property>
<property type="string" id="targetPythonClass" mode="w" >
ZenPacks.community.OracleMon.OracleTablespace
